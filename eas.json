{
  "cli": {
    "version": ">= 9.0.8",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    },
    "dapp-store": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "996569075"
      }
    }
  }
}
